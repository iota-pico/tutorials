{
    "compilerOptions": {
        "target": "es2017",
        "lib": [
            "es2015",
            "dom"
        ],
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "strictNullChecks": false,
        "esModuleInterop": true,
        "stripInternal": true,
        "experimentalDecorators": true
    }
}